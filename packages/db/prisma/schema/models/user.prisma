model User {
  id            String   @id
  name          String
  email         String   @unique()
  emailVerified Boolean  @map("email_verified")
  image         String?
  createdAt     DateTime @default(now()) @map("created_at")
  updatedAt     DateTime @updatedAt @map("updated_at")

  sessions Session[]
  accounts Account[]
  folders  Folder[]
  notes    Note[]

  @@map("user")
}
