generator client {
  provider = "prisma-client"
  output   = "../../generated/prisma"
  runtime  = "bun"
}

generator zod {
  provider = "prisma-zod-generator"
  output   = "../../generated/zod"
  config   = "../zod-gen.json"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
